<?xml version="1.0"?>

<PropertyList>

 <path>RJNY-tower-hanger.ac</path>

  <animation>
    <type>select</type>
      <object-name>red</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.53</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>material</type>
      <object-name>red</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.53</value>
      </greater-than>
    </condition>
    <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

</PropertyList>
